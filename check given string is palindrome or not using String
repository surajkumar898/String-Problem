class pal{
    public static String palindrome(String s){
        String s1="";
        int i; 
        for( i=s.length()-1; i>=0; i--){
         s1=s1+s.charAt(i);
        }
         if(s.equals(s1)){
         return "Palindrome";
        }
    
   return "Not Palindrome";
}  
public static void main(String[] args){
    String s = "747";
    String result = palindrome(s);
    System.out.println(result);
}
}
